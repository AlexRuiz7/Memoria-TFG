\newglossaryentry{dispersive}{name={Dispersive}, text={Dispersive},description={Regarding a material, and normally referred to frequency, it implies variations of its behaviour along with frequency}}

\newglossaryentry{SPAG}{name={Single Page Application}, text={Single Page Application},description={Web site which, once downloaded, allows dynamic interaction with the user by rewriting the page instead of requesting the whole page to the server each time}}

\newglossaryentry{websockets}{name={Websockets}, text={Websockets},description={Full-duplex communications protocol which allows multiple channles over a single \acrshort{TCP} connection (ports 80 or 443). Although it is not mandatory, it is usually used along with \acrshort{HTTP}. It enables interaction with a web site (typically a \acrshort{SPA}) with low overhead, easing real-time data transfer in both ways \cite{wiki}}}


\newglossaryentry{stakeholders}{name={Stakeholders}, text={Stakeholders},description={According to \acrshort{ISO}21500, person, group or organization that has interests in, or can affect, be affected by, or perceive itself to be affected by, any aspect of the project}}

\newglossaryentry{jitter}{name={Jitter}, text={Jitter},description={Deviation from presumably periodicity in relation to a reference clock signal}}


\newglossaryentry{torque}{name={Torque}, text={torque},description={Rotational equivalent of linear force}}

\newglossaryentry{biastee}{name={Bias-Tee}, text={Bias-Tee},description={Three-Port Network widely used to set the DC bias point in electronics devices, such as mixers or amplifiers}}


\newglossaryentry{rbudget}{name={Radio-Link Budget}, text={Radio-Link Budget},description={In a \acrshort{RF} design, accounting of the gains and losses which occur through the system, i.e., transmitter, medium and receiver}}


\newglossaryentry{payload}{name={Payload}, text={Payload},description={Carrying capacity of an aircraft, normally measured in terms of weight. More specifically, it can be referred to the equipment carried for the performance of a certain mission, e.g. an camera or an star tracker}}


\newglossaryentry{1dbcomp}{name={1 dB Compression Point}, text={1 dB Compression Point},description={Most nonlinear devices, such as mixers or amplifiers, tend to become lossier with increasing input power. At a certain power level, the gain response of the device will become reduced by 1 dB; that point is known as 1 dB compression point and it is expressed as a function of input or output power depending on the device}}

\newglossaryentry{quasitem}{name={Quasi-TEM Mode}, text={Quasi-TEM Mode},description={Normally  referenced when using \glsname{microstrip}, name given to the phenomena which occurs in that technology, where two different mediums (substrate and air), coexist. It differs from the TEM, among others things, in the propagation constant, which depends on \glsname{permittivity}}}

\newglossaryentry{h264}{name={MPEG-4/H.264}, text={MPEG-4/H.264},description={Video compression standard format, one of the most used for recording and distribution of video content. It is actually composed of a group of standards, with different profiles}}

\newglossaryentry{ground}{name={Ground Station}, text={Ground Station},description={Facilities in which instruments and devices necessary to establish a radio~-~link communication are normally located. Also used to control and monitor antenna system}}

\newglossaryentry{faraday}{name={Faraday Rotation}, text={Faraday Rotation},description={In satellite radio-links, phenomena due to Earth's Magnetic Field which implies a certain rotation when a linear polarized signal goes through ionosphere. Negligible above 10 GHz}}
\newglossaryentry{solid}{name={SolidWorks\textsuperscript{\textregistered}}, text={SolidWorks\textsuperscript{\textregistered}},description={CAD Software from Dessault Systèmes for 3D Mechanical Design}}

\newglossaryentry{farfield}{name={Far Field}, text={Far Field},description={Regarding Antenna Propagation, region of the electromagnetic field which occurs at enough distance from the antenna and in where the radiation pattern does not change with distance}}

\newglossaryentry{DATV}{name={DATV}, text={DATV},description={Also known as Digital Amateur TeleVision, it allows transmitting video signals with less bandwidth in comparison with others transmission schemes. It applies \acrshort{DVB-S} system, which is widely used in Europe}}

\newglossaryentry{genesys}{name={Keysight Genesys RF\textsuperscript{\textregistered}}, text={Keysight Genesys RF\textsuperscript{\textregistered}},description={RF and microwave \acrshort{EDA} software for circuit synthesis and simulation, from filters to transmission lines or \acrshort{PLL}}}
\newglossaryentry{iloss}{name={Insertion Losses}, text={Insertion Losses},description={Loss of signal power as a consequence of going into a transmission line or optical fiber. Usually measured in dB}}

\newglossaryentry{effarea}{name={Effective Area}, text={Effective Area},description={Ratio between received power and incident power density at the receiving antenna}}

\newglossaryentry{srate}{name={Symbol Rate}, text={Symbol Rate},description={Also known as baud rate, it refers to the number of symbols or waveform changes per time unit, across a transmission medium. It is measured in baud or symbols per second}}

\newglossaryentry{gbouncing}{name={Ground~Bouncing}, text={Ground~Bouncing},description={Phenomena which occurs when there is an inductance from the ground pin on an Integrated Circuit to a common ground for the system. IC switching provokes the current to flow through this inductance and makes the ground reference of the IC become higher than the global ground reference}}


\newglossaryentry{dropout}{name={Dropout Voltage}, text={Dropout Voltage},description={Regarding voltage regulators, minimum voltage drop required across the regulator to maintain output voltage}}

\newglossaryentry{boutline}{name={Border Outline}, text={Border Outline},description={In a \acrshort{PCB}, said of the boundary or extents of the board}}

\newglossaryentry{differential}{name={Differential Impedance}, text={Differential Impedance},description={Said of the ratio derived from the change in voltage between two ports and the change in current}}

\newglossaryentry{varactor}{name={Varactor}, text={Varactor},description={Also known as \textbf{varicap}, it is a type of variable diode, depending on capacitance. It is usually used as voltage-controlled capacitor}}

\newglossaryentry{baseband}{name={Baseband}, text={Baseband},description={Signal whose spectral magnitude is non-zero only in the nearby of the origin of frequencies and negligible elsewhere}}

\newglossaryentry{directivity}{name={Directivity}, text={Directivity},description={Regarding antenna parameters, it measures the power density radiated in the direction of the strongest emission, when compared with the power density radiated by an ideal \glsname{Isotropic} radiator radiating the same power}}

\newglossaryentry{losses}{name={Reflection Losses}, text={Reflection Losses},description={They occur on a medium when part of the energy is reflected back to the emitter. It may happen in different mediums, e.g., a transmission line}}

\newglossaryentry{permittivity}{name={Relative Permittivity}, text={Relative Permittivity},description={Regarding a material, said of its permittivity expressed as a ratio relative to permittivity of vacuum}}

\newglossaryentry{microstrip}{name={Microstrip Technology}, text={Microstrip Technology},description={Planar transmission line in which a conductor of width $W$ is printed on a thin, grounded dielectric substrate of thickness $d$ and \glsname{permittivity} $\epsilon_{r}$}}

\newglossaryentry{noise}{name={Noise Floor}, text={Noise Floor},description={Measure of the signal created from the sum of all undesired signals and noise sources within a measurement system}}

\newglossaryentry{PLF}{name={PLF}, text={PLF},description={Polarization Losses Factor. They occur when receiving and transmitting antennas are not equally polarized, or their polarizations are not of high quality. Depending on the type of mismatching, the amount of losses will vary}}

\newglossaryentry{S-Band}{name={S-Band}, text={S-Band},description={Official designation given by the \acrshort{IEEE} to a part of the microwave band of electromagnetic spectrum. It covers the range from 2 GHz to 4 GHz. It is widely used by radars, satellite radio-links and also for the \acrshort{ISM} band}}

\newglossaryentry{Isotropic}{name={Isotropic}, text={Isotropic},description={Regarding antennas, it is an ideal type of those which radiate< the same intensity of radiation in all directions. It is not physically feasible}}


\newglossaryentry{selectivity}{name={Selectivity}, text={Selectivity},description={Measure of the performance of a receiver to receive only the radio signal for which it has been tuned, rejecting others signals nearby in frequency}}

\newglossaryentry{AxialRatio}{name={Axial Ratio}, text={Axial Ratio},description={Ratio of the orthogonal components of an E-field. For circularly polarized antennas, ideal value is 0 dB. In case of linear polarization, it tends to infinite}}
\newglossaryentry{Broadside}{name={Broadside}, text={Broadside},description={Regarding radiation pattern, an antenna is said to be working at broadside direction when the maximum radiation direction is transversal to the axis}}

\newglossaryentry{telecommand}{name={Telecommand}, text={Telecommand},description={Command sent to remotely control a system which is not directly connected to the controller system. It is the counterpart of the \glsname{telemetry}}}

\newglossaryentry{telemetry}{name={Telemetry}, text={Telemetry},description={Process by which measurements or any other relevant information is sent automatically or on-demand to a remote system. It is commonly referred to wireless transfer mechanism, specially in space. It is the counterpart of the \glsname{telecommand}}}

\newglossaryentry{Raspberry Pi HAT}{name={Raspberry Pi HAT},description={Nombre común de las extensiones \textit{hardware} de la comunidad de la plataforma Raspberry Pi. Hace referencia a\textit{ Hardware Attached on Top} y juega con el doble sentido de la palabra \textit{hat} o sombrero en inglés}}

\newglossaryentry{API HTTP REST}{name={API HTTP REST},description={Siglas de Application Programming Interface, HiperText Tranfer Procotol y REpresentational State Transfer. Este concepto hace referencia a un tipo de interfaz web hacia el lado del cliente basada en \acrshort{HTTP} que utiliza los verbos comunes del protocolo HTTP (GET, POST, DELETE...) para acceder y gestionar datos e información existente en el lado del servidor}}

\newglossaryentry{HTTP Websockets}{name={HTTP Websockets},description={Tecnología que permite generar una comunicación \textit{full-duplex} sobre el protocolo de aplicación \acrshort{HTTP} similar a la utilizada por \acrshort{TCP} en la capa de transporte de la pila \acrshort{OSI}}}

\newglossaryentry{MVC}{name={Modelo-Vista-Controlador},description={Paradigma de arquitectura de \textit{software} que separa la lógica del programa del conjunto de datos a manejar, así como de la representación de los mismos. Tal como indica el propio nombre, los tres pilares de eta arquitectura son el modelo de datos, el controlador o lógica de programa y la vista o interfaz de usuario}}

\newglossaryentry{RWD}{name={Responsive Web Design},description={Filosofía de diseño web orientada a hacer diseños adaptables al dispositivo en el que se muestre la web en cuestión. Se tienen en cuenta tanto el tamaño, resolución y ratio de aspecto de la pantalla como el tipo de interfaz de interacción de usuario (ratón, táctil...)}}

\newglossaryentry{FSJ}{name={Full Stack JavaScript},description={Tambien conocido como \acrshort{MEAN} Stack, es un nuevo paradigma de programación web que está sustituyendo al famoso \acrshort{LAMP}. Se basa en el uso de JavaScript y \acrshort{JSON} desde la interfaz de usuario hasta el servidor y la base de datos. Pretende unificar el lenguaje de programación web así como la notación de objetos}}

\newglossaryentry{daemon}{name={Daemon},text={daemon},description={Se conoce como servicio o proceso \textit{daemon} a aquellos procesos que se ejecutan en segundo plano y no tienen interacción con el usuario dentro del conjunto de procesos de un sistema operativo. En programación multihebrada en Python, se conoce como hilo \textsl{daemon} a aquellos hilos secundarios que se ejecutan en segundo plano y que terminan junto con el hilo de ejecución principal, sin necesidad de tener que gestionar su estado desde el mismo}}

\newglossaryentry{ruido acustico}{name={Ruido acústico},description={Se considera ruido acústico toda aquella potencia emitida en forma de onda acústica que no es deseada en un entorno concreto para un cierto grupo de receptores}}

\newglossaryentry{variables ambientales}{name={Variables ambientales},text={variables ambientales},description={En el contexto de este proyecto, se consideran variables ambientales todas aquella características que describen el estado de un sistema ambiental, entendiendo por sistema ambiental un entorno urbano, rural, un local o un emplazamiento público. Son variables ambientales la temperatura, la humedad, la luminosidad o la velocidad del viento o el ruido acústico, entre otras}}

\newglossaryentry{sensor}{name={Sensor},description={En el contexto de este proyecto, se considera un sensor a todo aquel dispositivo capaz de medir el estado de las \glsname{variables ambientales} en un instante concreto}}

\newglossaryentry{altium}{name={Altium Designer\textsuperscript{\textregistered} 19},description={\acrshort{EDA} software used to design \acrshort{PCB} from schematics. It allows 3D Design, as well as electronics simulation}}

\newglossaryentry{RSA}{name={RSA},description={RSA (Rivest, Shamir y Adleman) hace referencia al primer y más común sistema criptográfico de clave pública. Mediante un par de claves, una pública y otra privada, es posible encriptar y firmar digitalmente información. Se basa en la idea de que todo lo que se cifre con una de las claves solo podrá ser descrifrado con la otra, y en que la clave privada únicamente la conocerá el dueño de las claves, mientras que la clave pública será accesible por cualquiera interesado}}

\newglossaryentry{formato PEM}{name={formato PEM},description={Privacy-enhanced Electronic Mail es un formato abierto para el almacenado de claves \glsname{RSA}. Utiliza codificación Base64 y está mantenido por la \acrshort{IETF}}}
